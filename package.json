{
  "name": "@build-ts/swagger-codegen",
  "version": "1.2.3",
  "description": "Generate TypeScript models, endpoints, axios config and React hooks from Swagger/OpenAPI specs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swagger-codegen": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist test-output",
    "test": "yarn build && node dist/cli.js https://petstore.swagger.io/v2/swagger.json -o ./test-output",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "codegen",
    "api",
    "axios",
    "react",
    "react-hooks",
    "code-generator",
    "api-client",
    "fetch"
  ],
  "author": "Ali",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/build-ts/swagger-codegen.git"
  },
  "bugs": {
    "url": "https://github.com/build-ts/swagger-codegen/issues"
  },
  "homepage": "https://github.com/build-ts/swagger-codegen#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "axios": ">=0.27.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.az.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
